{
  "schemaVersion": 1,
  "id": "expanded_axe_enchanting",
  "version": "${version}",
  "name": "Expanded Axe Enchanting",
  "description": "This mod allows you to use Fire Aspect, Knockback, Looting, Impaling, Density, Breach, and Wind Burst on an axe.",
  "authors": [
    "SmushyTaco"
  ],
  "contact": {
    "homepage": "https://www.smushytaco.com/",
    "issues": "https://www.curseforge.com/minecraft/mc-mods/expanded-axe-enchanting/issues/",
    "sources": "https://github.com/SmushyTaco/Expanded-Axe-Enchanting/"
  },
  "license": "Apache-2.0",
  "icon": "assets/expanded_axe_enchanting/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.expanded_axe_enchanting.ExpandedAxeEnchanting"
      }
    ]
  },
  "mixins": [
    "expanded_axe_enchanting.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-api": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}",
    "owo": ">=${owo_version}"
  }
}